{
  "name": "hackathon",
  "displayName": "Hackathon",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hackathon.helloWorld",
        "title": "Hello World"
      }
    ],
    "chatParticipants": [ 
            {
                "id": "testcase-error-finder",
                "name": "errorfinder",
                "fullName": "Test Case Error Finder",
                "description": "Find errors in test cases and create Jira tickets",
                "isSticky": true,
                "commands": [
                {
                    "name": "finderrors", 
                    "description": "Analyze test cases and find errors"
                },
                {
                    "name": "createJiraTicket", 
                    "description": "Create a Jira ticket for the detected error"
                }
            ]
            }
        ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.54.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
